.navbar.bg-base-300.bg-opacity-50.sticky.top-0.backdrop-blur-md.z-40
  .flex-1
    div.btn.btn-ghost.text-xl.cursor-default#logo
      img.h-8.w-8.logo-light(src="/paste/assets/logo-light.svg" alt="logo light" loading="lazy")
      img.h-8.w-8.logo-dark(src="/paste/assets/logo-dark.svg" alt="logo dark" loading="lazy")
      span Paste&nbsp;
      
  .flex-none.gap-2
    div.btn.btn-ghost.btn-circle
      a.tooltip.tooltip-bottom(href="https://github.com/nozsh/zeta-paste" data-tip="GitHub" aria-label="GitHub")
        +mdi-github("h-5 w-5")
    div.btn.btn-ghost.btn-circle
      a.tooltip.tooltip-bottom(href="about/" data-tip="Everything to know" aria-label="About" id="about")
        +mdi-about("h-5 w-5")
    div
      include theme-control.pug